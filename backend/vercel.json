{
  "version": 2,
  "builds": [
    {
      "src": "server.js",  // Tên file chính của ứng dụng Express
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"  // Chuyển hướng tất cả yêu cầu đến file server.js
    }
  ],
  "env": {
    "NODE_ENV": "production", // Cấu hình biến môi trường nếu cần
    "OTHER_ENV_VARIABLE": "value" // Thêm biến môi trường khác nếu cần
  }
}
